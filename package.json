{
  "name": "tractor-plugin-cucumber",
  "version": "0.3.8",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-plugin-cucumber.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-plugin-cucumber/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean && npm run lint && npm run cover && npm run compile",
    "clean": "rimraf dist",
    "compile": "babel src --out-dir dist/ --ignore *.spec.js",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src/**/*.js; true",
    "test": "mocha --opts ./.mocharc 'src/**/*.spec.js'",
    "test:debug": "mocha --inspect --debug-brk --opts ./.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "dedent": "^0.7.0",
    "dirty-chai": "^2.0.1",
    "escodegen": "^1.8.1",
    "eslint": "^4.8.0",
    "esprima": "^4.0.0",
    "ineeda": "^0.11.0",
    "istanbul": "1.0.0-alpha.2",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "npm-watch": "^0.3.0",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0",
    "tractor-config-loader": "^0.4.4",
    "tractor-error-handler": "^0.2.4",
    "tractor-file-structure": "^0.5.4",
    "tractor-logger": "^0.1.1"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "chai-as-promised": "^7.1.1",
    "cucumber": "3.0.5",
    "cucumber-html-reporter": "0.3.5",
    "graceful-fs": "^4.1.11",
    "protractor-cucumber-framework": "^4.0.8",
    "tractor-plugin-features": "^0.2.8",
    "tractor-plugin-step-definitions": "^0.2.11"
  },
  "peerDependencies": {
    "tractor": "^0.12.1"
  }
}
